{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "./fre-jsx-runtime"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "jsxImportSource": "fre",
    "baseUrl": "./",
    "sourceMap": true,
    "paths": {
      "~/*": ["./src/*"],
      "~/public/*": ["./public/*"]
    }
  },
  "outDir": "./dist",
  "include": [
    "./src"
  ]
}
